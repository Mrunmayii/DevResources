<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.devlist.ui.fragments.HomeFragment"
        android:label="home_fragment"
        tools:layout="@layout/home_fragment" >
        <action
            android:id="@+id/action_homeFragment_to_publicApiFragment"
            app:destination="@id/publicApiFragment" />
        <action
            android:id="@+id/action_homeFragment_to_uiFragment"
            app:destination="@id/uiFragment" />
        <action
            android:id="@+id/action_homeFragment_self"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_homeFragment_to_assetsFragment"
            app:destination="@id/assetsFragment" />
    </fragment>
    <fragment
        android:id="@+id/publicApiFragment"
        android:name="com.example.devlist.ui.fragments.PublicApiFragment"
        android:label="public_api_fragment"
        tools:layout="@layout/public_api_fragment" >
        <action
            android:id="@+id/action_publicApiFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/uiFragment"
        android:name="com.example.devlist.ui.fragments.UiFragment"
        android:label="ui_fragment"
        tools:layout="@layout/ui_fragment" >
        <action
            android:id="@+id/action_uiFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/assetsFragment"
        android:name="com.example.devlist.ui.fragments.AssetsFragment"
        android:label="assets_fragment"
        tools:layout="@layout/assets_fragment" >
        <action
            android:id="@+id/action_assetsFragment_to_iconsFragment2"
            app:destination="@id/iconsFragment" />
        <action
            android:id="@+id/action_assetsFragment_to_fontFragment"
            app:destination="@id/fontFragment" />
    </fragment>
    <fragment
        android:id="@+id/iconsFragment"
        android:name="com.example.devlist.ui.fragments.IconsFragment"
        android:label="IconsFragment" >
        <action
            android:id="@+id/action_iconsFragment_to_assetsFragment"
            app:destination="@id/assetsFragment" />
    </fragment>
    <fragment
        android:id="@+id/fontFragment"
        android:name="com.example.devlist.ui.fragments.FontFragment"
        android:label="FontFragment" >
        <action
            android:id="@+id/action_fontFragment_to_assetsFragment"
            app:destination="@id/assetsFragment" />
    </fragment>
    <fragment
        android:id="@+id/programmingFragment"
        android:name="com.example.devlist.ui.fragments.ProgrammingFragment"
        android:label="ProgrammingFragment" >
        <action
            android:id="@+id/action_programmingFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/UTubeFragment"
        android:name="com.example.devlist.ui.fragments.UTubeFragment"
        android:label="UTubeFragment" >
        <action
            android:id="@+id/action_UTubeFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/uiKitFragment"
        android:name="com.example.devlist.ui.fragments.UiKitFragment"
        android:label="UiKitFragment" >
        <action
            android:id="@+id/action_uiKitFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>